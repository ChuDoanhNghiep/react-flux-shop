.ie8
  img
    max-width none  
  .area-container
    width 980px

  .main-header 
    height 40px
    .wishlist 
      >.title.img
        padding 11px
      .icon-heart-base64
        // background-image url("data:image/png;base64,")
        // display none
        background-image url("data:image/png;base64,")
      // .icon-heart-white-ie8
      //   display inline
      //   vertical-align middle
    .register-menu
      >.list
        >.wishlist.li
          >.img.title
            padding 10px 11px 10px
            
      >.tabs 
        >.tab 
          >.content 
            font-size 9px

    .icon-shoppingbag-white-base64
      background-image url("data:image/png;base64,")
    .icon-search-base64
      background-image url("data:image/png;base64,") 
    
    &.sticky-header
      height 54px
      .register-menu
        .register,
        .sign-in
          display none
        >.list 
          >.li.wishlist 
            >.title.img
              padding 17px 11px 17px
          >.li.shopping-bag
            >.title.img
              padding 13.5px 10px
          >.li.desktop-search 
            .img
              padding-top 19px
        .desktop-searchbox 
          .placeholderWrap
            top 21px
          input
            padding-top 19px
      .arrow
        border 6px solid transparent
        border-bottom-color #fff 
        left 30px
        position absolute
        top 24px
        width 0
        z-index 3
        opacity 0
        display block
  .sticky-menu-opened
    .main-header.sticky-header
      .arrow
        filter none

  .register-menu.fashion.menu-tabs
    .tab
      >.title
        position static

      &:hover 
        >.title
          position static
          .icon-shoppingbag-white-base64
            background-image url("data:image/png;base64,")
          .icon-heart-base64 
            // background-image url("data:image/png;base64,")
            background-image url("data:image/png;base64,")
          // .icon-heart-white-ie8
          //   display none
          // .icon-heart-black-ie8
          //   display inline

  .sticky-header
    .sticky-menu
      display block
      margin-top 18.5px
      img
        margin-left 24px
    .header-logo
      display block
      margin-top 0px
      opacity 1
      -ms-filter none
      filter none
      

  .footer
    .footer-heading
      .arrowWrap
        top -10px

  .follow-us 
    .i-dark-facebook,  
    .i-gray-facebook
      margin-right 15px

    .i-dark-instagram, 
    .i-dark-sina, 
    .i-dark-youku,
    .i-dark-youtube,
    .i-gray-instagram,
    .i-gray-sina,
    .i-gray-youku,
    .i-gray-youtube
      margin-right 12px

  #mobile-menu
    position fixed
    top 54px
    left 20px
    z-index -1

  .sticky-menu-opened
    #mobile-menu
      z-index 99
      overflow-y auto

  &.no-touch 
    .fashion.menu-tabs 
      .tab:hover 
        >.content
          display block
        >.title
          border-left 1px solid #e1e1e1
          border-right 1px solid #e1e1e1
          border-top 1px solid #e1e1e1
          background #fff
          color #000
          a 
            color #000
  
  //product details page
  .productDetails
    .details 
      .actions 
        .icon-heart-base64
          background-image url("data:image/png;base64,")

    //subcategory page
    .icon.icon-heart 
