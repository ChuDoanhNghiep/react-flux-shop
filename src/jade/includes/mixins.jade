mixin icon(value)
  - var style = attributes.style ? attributes.style : ''
  div(class="icon-"+value, style!=style)
    //- img(src="http://placehold.it/16x15")

mixin socialIcon(value)
  //- span(class="icon icon-"+value)
  img(class="i-dark-"+value+" noHover", src="./images/icons/"+value+"_black_2x.png", style="height:20px")
  img(class="i-gray-"+value+" hover", src="./images/icons/"+value+"_gray_2x.png", style="height:20px")

mixin flag(value)
  - var classes = attributes.class ? attributes.class : ''
  //- div(class="icon-flag-"+value)
  img(class!=classes, src="./images/flags/flag-"+value+".png", alt!=value, width="22px", height="14px")

mixin breadcrumb(links)
  <!-- breadcrumb -->
  nav.breadcrumb
    ul.list
      - each value, index in links
        if index == 0
          li.element
            a(href="#")=value
        else
          li.element.divider
            a(href="#")=value
  <!-- END breadcrumb -->

mixin input(type,name,text, id,checked)
  label.cfe(for=id? id : name) 
    if (checked)
      input.cfe-styled(type=type, name=name, id=id? id : name, value= type == 'radio'? id : '', checked)
    else  
      input.cfe-styled(type=type, name=name, id=id? id : name, value= type == 'radio'? id : '')
    !=text

mixin category(category)
  .list.expandable
    .title=category.name
    .content.submenu
      - for elem in category.subcategory
          a.title(href='#')=elem

mixin slider(nb, image, id)
  <!-- slider -->
  .slider.swipe(id=id ? id : '')
    .swipe-wrap
      if block
        block
      else
        - for(var i=0;i<nb;i++)
          .slide
            img(src='./static/img/promotions/slider-'+i+'.jpg', alt="slide "+i)
    .swipe-controls.bullets.kiwi-hidden-s
        div
          - for(var i=0;i<nb;i++)
            a.bullet(href="#" index=i, class=i==0? 'active' : '')
    .swipe-controls
      .prev
        i.icon-arrow-left
      .next
        i.icon-arrow-right

  <!-- END slider -->


mixin customSelect(select)
  .custom-select
    .dropdown-text
      span=select.options[select.current].name
    .dropdown-list
      - each opt, index in select.options
        .item(class=(index == select.current)? 'active' : '')
          a(href="#", data-value=opt.value)=opt.name
    input(type="hidden", value=select.options[select.current].value)

mixin customSelectLabel(select)
  .custom-select
    .dropdown-text
      span=select.label
    .dropdown-list
      - each opt, index in select.options
        .item(class=(index == select.current)? 'active' : '')
          a(href="#", data-value=opt.value)=opt.name
    input(type="hidden")

mixin customLinkSelect(select)
  .custom-Link-select.custom-select
    .dropdown-text=select.name
    .dropdown-list
      - each opt in select.options
        .item
          a(href="#")=opt


mixin myTabs(tabs, border)
  .tabs-title.kiwi-hidden-s
    each val in tabs 
      a(href="#",class!=val.active? 'active':'').title-item=val.title

  if border
    .border-cover.kiwi-hidden-s

mixin myNavTabs(tabs)
  .tabs-title.kiwi-visible-s
    each val, idx in tabs
      if idx == 0
        a(href="#",class="tab"+idx).title-item.active=val
      else
        a(href="#",class="tab"+idx).title-item=val
  .border-cover.kiwi-visible-s

mixin wildcard(color)
  span(style='color:'+color) *

mixin toggleButton(cked)
  .togglebox
    if cked
      input(type="checkbox", name="iosbutton", id="iosbutton", checked="true")
    else
      input(type="checkbox", name="iosbutton", id="iosbutton")
    
    label(for="iosbutton")

mixin Menu(type, current)
  if type == 'mobile'
    +menu-mobile(current)
  else if type == 'mobile-loggedin' 
    +menu-mobile-login(current)
  else if type == 'mobile-shop'
    +menu-mobile-shop(current)
  else
    +menu-desktop(current)
  
