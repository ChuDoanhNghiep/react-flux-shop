mixin product(product)
  div(class=product.slider ? '' : 'kiwi-col l-1-3 m-1-3 s-1-2')
    a.productLink(href="product-detail.html")
      .product
        .picture
          img.opacity-0.fade-in(src="http://placehold.it/200", alt=product.name, onload="$(this).css('opacity', '1');")
        
        .detail
          .name
            .brand-name=product.brand
            .product-name=product.name
          if product.extra != 'out-of-stock'
            if product.discountPrice
              +price(product.price)(class="before-discount")
              +price(product.discountPrice, true)(class="after-discount")
            else
              +price(product.price, true)(class="no-discount")
          if product.extra == 'out-of-stock'
            .extra Out of stock
          else if product.extra
            .extra.primary!=product.extra

        .description=product.desc

        .actions
          .action.wishlist(class=product.wishlist? 'active' : '', data-tip=product.wishlist? "This is on your Wish List" : "Add to Wish List", data-tip-effect='product-tip-fade', data-productid='{000F330F-8B18-46C2-A92B-D22300E52FB5}') 
            i.icon.icon-heart.grey

          .action.quickview(href="./product-detail.html", data-mfp-src="#modal-quick-view", data-effect="mfp-fade-in", data-tip="Quick Look", data-tip-effect='product-tip-fade') 
            i.icon.icon-quicklook.black
        
        if product.badge
          .badge(class!=product.badgeClass?product.badgeClass:'')=product.badge

mixin price(price, haveTooltip)
  - var classes = attributes.class ? attributes.class : ''
  .price(class!=classes)
    if haveTooltip
      span.currency(data-tip="currency-tip", data-tip-position='left', data-tip-effect='currency-tip')
        span.underline S$
        span.amount=price
        .tooltip
          .currenciesList
            p Exchange rates are 
              br 
              | for reference only
            ul
              +currency('hongkong', 'hk', '$', price, 6.20639)
              +currency('singapore', 'us', '$', price, 0.800617)
              +currency('australia', 'eu', '€', price, 0.586707)
              +currency('japan', 'jp', '£', price, 0.470220)
              +currency('newzealand', 'rm', '¥', price, 4.96957)
    else
      span.currency S$      
      span.amount=price

mixin currency(icon, country, symbol, price, change)
  li
    span.country
      +flag(icon)(class='icon-prependInline')
      | #{country}
    span.symbol #{symbol}
    - var newPrice = Math.round(price*change*100)/100
    span.value=newPrice

mixin pictureSlider(nb, quickview, enlarge)
  .images
    .badge Lorem ipsum
    if enlarge
      .zoom-message.kiwi-visible-m.kiwi-visible-s 
        img.zoom-icon(src="/static/img/zoom-plus-x2.png")
    .primary.slider.swipe
      .swipe-wrap
        - for (var x = 0; x < nb; x++)
          .slide
              div(class= quickview ? '' : 'component magnify')
                img.target.opacity-0.fade-in(src="http://placehold.it/390&text=small+"+x, big="http://placehold.it/780&text=large+"+x, onload="$(this).css('opacity', '1');")
                if !quickview
                  .magnifier(style="width: 180px; height: 180px; left: 0px; top: 122px;")
      .swipe-controls.bullets.kiwi-visible-m
        div
          - for(var i=0;i<nb;i++)
            a.bullet(href="#" index=i, class=i==0? 'active' : '', data-slide=i)

      .swipe-controls.navs.kiwi-visible-m
        .nav.prev
          .icon-arrow-left(data-slide='prev')
        .nav.next
          .icon-arrow-right(data-slide='next')
    .thumbnails.sliderWrap.kiwi-hidden-m.kiwi-hidden-s
      .thumbnails-slider.slider
        .slides
          - for (var x = 0; x < nb; x++)
            .slide
              img.opacity-0.fade-in(src="http://placehold.it/60x80&text=thumb+"+x, data-slide=x, onload="$(this).css('opacity', '1');")

mixin productDetail(product)
  section.product.productDetails(class=product.quickview ? '' : 'magnifier', id=product.enlarge ? 'tap-enlarge' : '')
    .kiwi-grid
      .kiwi-col.l-1-2.m-1   
        if !product.quickview 
          +preorder-productmessage()        
        +pictureSlider(5,product.quickview,product.enlarge)
        
      .kiwi-col.l-1-2.m-1
        .details
          .reference
            .brand=product.brand
            .name=product.name
            .code=product.code
          
          if product.discountPrice
            +price(product.price)(class="before-discount")
            +price(product.discountPrice, true)(class="after-discount")
          else
            +price(product.price, true)(class="no-discount")

          .custom
            .kiwi-grid
              .kiwi-col.l-1.m-1-2
                if product.size
                  .custom-select.size
                    .dropdown-text=product.size[product.size.active]
                    .dropdown-list
                      each size,idx in product.size
                        if idx != 'active'
                          .item(class=idx==product.size.active ? 'active':'')
                            a(href="#", data-value=idx)=size

                    input(type="hidden", name="size", value=product.size.active)
              .kiwi-col.l-1.m-1-2
                .quantity 
                  span.label Quantity
                  span.qt-choice.minus -
                  span.qt-choice.qt-value 1
                  span.qt-choice.plus +

          .actions
            .kiwi-grid
              .kiwi-col.l-1-2.m-1
                .action.addToShoppingbag
                  a.button.action.expand(class=(product.extra == 'out-of-stock')? 'disabled':'')
                    +icon('shoppingbag-white icon-prependInline')
                    | Add to shopping bag
              .kiwi-col.l-1-2.m-1
                .action.addToWishlist
                  a.button.black.expand(data-tip="Save to Wish List to create your personal list of products and gifts", data-tip-position="top")
                    +icon('heart-base64 icon icon-prependInline')
                    | Add to wish list
          if product.extra == 'out-of-stock'
            .extra Sorry, this item is currently out of stock, but will be back later.
          else
          .extra.primary
            +icon('pricetag icon-prependInline')
            | Buy 2, get 1 free
          .availability
            +icon('pin-gray icon-inline')
            | This product is currently available at a DEMO store. 
            a.underline(href="#") Find it now
          .description.accordion-menu
            .list.expandable.expanded
              .title Details
              .content=product.desc
            .list.expandable
              .title Details
              .content=product.desc
            .list.expandable
              .title Details
              .content=product.desc
          .more(class=product.quickview ? '' : 'hidden') 
            a(href='product-detail.html') See more Details ›
          .share
            .kiwi-grid
              .kiwi-col.l-1-2.m-1
                .asking(class=product.quickview ? 'hidden' : '') Have a question? 
                  a.underline(href="#") Email us
              .kiwi-col.l-1-2.m-1
                .share-button share:
                  a(href="#") facebook                